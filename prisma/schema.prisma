datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    output   = "../node_modules/.prisma/client"
}

model User {
    id       String @id @db.ObjectId @map("_id")
    fullName String
    email    String @unique
    phone    String
}
